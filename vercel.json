{
    "version": 2,
    "builds": [
      {
        "src": "spentbackend/manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "installCommand": "pip install -r requirements.txt",
          "startCommand": "python manage.py runserver"
        }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/spentbackend/static/$1" },
      { "src": "/media/(.*)", "dest": "/spentbackend/media/$1" },
      { "src": "/(.*)", "dest": "/spentbackend/manage.py" }
    ]
  }
  