{
    "version": 2,
    "builds": [
      {
        "src": "spentbackend/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build"        
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "spentbackend/wsgi.py" }
    ]
  }
  